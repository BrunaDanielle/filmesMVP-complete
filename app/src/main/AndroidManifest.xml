<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.filmesmvp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icone_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".filmesmvp.Register.ResetPasswordActivity"></activity>
        <activity android:name=".filmesmvp.Register.LoginActivity" />
        <activity android:name=".filmesmvp.filmes.MainActivity" />
        <activity
            android:name=".filmesmvp.filmes.FilmeDetalhesActivity"
            android:theme="@style/AppTheme2" />
        <activity android:name=".filmesmvp.Register.SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>